{
  "manifest_version": 3,
  "name": "Practice Chrome Extension",
  "version": "1.0.0",
  "description": "A simple React app as a Chrome extension",
  "action": {
    "default_icon": "/icons/logo.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "declarativeNetRequest",
    "storage",
    "windows"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [{
    "resources": ["index.html", "/icons/*"],
    "matches": ["<all_urls>"]
  }]
}